<% if flash[:errors] %>
stop_spinner();
if (!$('.error').length)
    {
        $('.modal #password').after('<small class="error">Incorrect Password</small> ');
    }
$('.modal .row').addClass('error');
<% else %>
<% flash[:message] = "Account Deleted Successfully" %>
window.location.replace('<%= root_url %>');
<% end %>